from "generics/capacitors.ato" import Capacitor
from "nfc/NT3H1101W0FHKH.ato" import NT3H1101W0FHKH
from "nfc/HMNFC.ato" import HMNFC_Class6

module Card:
    signal GND
    signal VCC

    rf = new HMNFC_Class6

    cap = new Capacitor
    cap.footprint = "C0402"
    cap.value = 50pF
    cap.p1 ~ rf.LA
    cap.p2 ~ rf.LB

    nfc = new NT3H1101W0FHKH
    nfc.VSS ~ GND
    nfc.VCC ~ VCC
    nfc.LA ~ cap.p1
    nfc.LB ~ cap.p2


